{
  "name": "@mcpvals",
  "version": "0.0.1",
  "description": "Evaluation library for Model Context Protocol servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "bin": {
    "mcpvals": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.mjs",
      "require": "./dist/next/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "directory": ".",
    "type": "git",
    "url": "git+https://github.com/mcpvals/mcpvals.git"
  },
  "bugs": {
    "url": "https://github.com/mcpvals/mcpvals/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "version-packages": "changeset version",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "mcp",
    "evaluation",
    "testing",
    "llm",
    "ai"
  ],
  "author": "mcpvals",
  "license": "Apache-2.0",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "redis": "^5.5.6",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@types/node": "^22.15.8",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.1",
    "zod": "^3.25.50"
  },
  "peerDependencies": {
    "ai": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.4.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
